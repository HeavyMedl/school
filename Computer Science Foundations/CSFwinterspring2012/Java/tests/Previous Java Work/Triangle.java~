public class Triangle
{
  private int width;
  
  public Trinagle(int aWidth)
  {
    width = aWidth;
  }
  
  public String toString()
  {
    String row = "";
    for (int i = 1; i <= width; i++)
    {
      for (int j = 1; j <= i; j++)
        r = r + "[]";
      r = r + "\n";
    }
    return r;
  }
}