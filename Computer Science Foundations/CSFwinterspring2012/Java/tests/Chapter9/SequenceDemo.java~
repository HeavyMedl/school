public class SequenceDemo
{
  public static void main(String[]args)
  {
    final int[] citypop = {1000,5666,1345,6534,6345,83456,134234,634578,
      5734, 5623, 7345, 45635, 63488, 734577 };
    Sequence mycity = new mySequence(citypop);
    LastDigitDistribution dist1 = new LastDigitDistribution();
    dist1.process(mycity, 1000);
    dist1.display();
    System.out.println();
  }
}
      
